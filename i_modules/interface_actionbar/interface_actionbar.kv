 # kivy 2.0.0


<CheckpointButton>:
    spacing: 5
    size_hint_x: 0.2

    Label:
        text: 'Save Checkpoint?'
        size_hint_x: 0.4

    CheckBox:
        padding: 2
        active: True
        size_hint_x: 0.05


<Terminate>:
	size_hint_x: 0.05
	text: 'X'


<TrainButton>:
	size_hint_x: 0.05
	text: '>'